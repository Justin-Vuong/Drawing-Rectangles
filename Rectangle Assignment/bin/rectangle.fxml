<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<VBox fx:id="vbox" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/9.0.4" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RectangleController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#close" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Rectangle fx:id="rectangle1" arcHeight="5.0" arcWidth="5.0" fill="#3e8f36" height="186.0" layoutX="107.0" layoutY="69.0" stroke="BLACK" strokeType="INSIDE" width="78.0" />
            <Rectangle id="rectangle2" fx:id="rectangle2" arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="66.0" layoutX="36.0" layoutY="153.0" stroke="BLACK" strokeType="INSIDE" width="200.0" />
            <Button fx:id="moveButton" layoutX="305.0" layoutY="299.0" mnemonicParsing="false" onAction="#move" text="Move" />
            <Button fx:id="ovelrlapButton" layoutX="221.0" layoutY="299.0" mnemonicParsing="false" onAction="#calculateOverlap" text="Overlap" />
      </children>
    </AnchorPane>
  </children>
</VBox>
